@using TicariOtomasyon.Models.Classes
@model List<SatisHareket>
    @{
        ViewBag.Title = "Siparislerim";
        Layout = "~/Views/Shared/_CariLayout.cshtml";
    }
    <br />
    <h2>Sipariş Geçmişim</h2>
    <br />

    <table class="table table-bordered">

        <tr>
            <th>Ürün Adı</th>
            <th>Tarih</th>


        </tr>

        @foreach (var x in Model)
        {
    <tr>
        <td>@x.Urunlers.UrunAd</td>
        <td>@x.Urunlers.SatisFiyat</td>
        <td>@Convert.ToDateTime(x.Tarih).ToString("dd/MM/yyyy")</td>

    </tr>
        }

    </table>
